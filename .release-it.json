{
  "github": {
  	"release": true,
    "releaseName": "v${version}",
    "assets": ["VERSION"]
  },
  "npm": {
    "publish": false,
    "ignoreVersion": true
  },
  "plugins": {
    "@release-it/bumper": {
      "in": {
        "file": "VERSION"
      },
      "out": {
        "file": "VERSION"
      }
    }
  }
}
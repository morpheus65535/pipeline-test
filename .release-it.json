{
  "github": {
  	"release": true,
    "preRelease": true
  },
  "npm": {
    "publish": false,
    "ignoreVersion": true
  },
  "plugins": {
    "@release-it/bumper": {
      "in": {
        "file": "VERSION"
      },
      "out": {
        "file": "VERSION"
      }
    },
    "@release-it/conventional-changelog": {
      "preset": "angular"
    }
  }
}
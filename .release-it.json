{
  "github": {
  	"release": true,
    "releaseName": "v${version}",
    "releaseNotes": "npx auto-changelog --config $GITHUB_WORKSPACE/.auto-changelog-$DEST_BRANCH"
  },
  "npm": {
    "publish": false,
    "ignoreVersion": true
  },
  "plugins": {
    "@release-it/bumper": {
      "in": {
        "file": "VERSION"
      },
      "out": {
        "file": "VERSION"
      }
    }
  }
}
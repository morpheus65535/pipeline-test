{
  "github": {
  	"release": true,
    "preRelease": true,
    "releaseName": "v${version}"
  },
  "npm": {
    "publish": false,
    "ignoreVersion": true
  },
  "plugins": {
    "@release-it/bumper": {
      "in": {
        "file": "VERSION"
      },
      "out": {
        "file": "VERSION"
      }
    }
  }
}
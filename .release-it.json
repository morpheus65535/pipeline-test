{
  "github": {
    "preRelease": true,
    "tokenRef": "${{ secrets.GITHUB_TOKEN }}"
  },
  "plugins": {
    "@release-it/bumper": {
      "in": {
        "file": "src/__init__.py",
        "type": "text/x-python"
      },
      "out": {
        "file": "src/__init__.py",
        "type": "text/x-python"
      }
    }
  }
}
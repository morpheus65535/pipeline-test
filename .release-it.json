{
  "github": {
    "preRelease": true
  },
  "npm": {
    "publish": false,
    "ignoreVersion": true
  },
  "plugins": {
    "@release-it/bumper": {
      "in": {
        "file": "src/__init__.py",
        "type": "text/x-python"
      },
      "out": {
        "file": "src/__init__.py",
        "type": "text/x-python"
      }
    }
  }
}